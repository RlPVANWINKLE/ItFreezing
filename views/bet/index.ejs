
<div style="text-align: center;">
  <h2 class="opac" style="margin: auto;">Existing Bets</h2>
<% bets.forEach(bet => { %>
<div class="Ebet">
  <h4 style="position: absolute; margin-top: 1%; margin-left: 1%;"><u><%=DOTW[bet.date]%></u></h4>
  <h1 style=" margin-top: 20%;"><%= bet.name %></h1>
  <h2 id="color" style=""><%= bet.bet %></h2>
</div>
<%}) %>
</div>
<div style="margin-top: 5%;">
  <a href="/bets/reset" class="button">RESET</a>
</div>